{
  "extends": [
    ":preserveSemverRanges",
    "group:all",
    "schedule:monthly",
    ":maintainLockFilesMonthly"
  ],
	"baseBranches": [
    "master",
    "react-redux-observable",
    "react-redux-thunk",
    "react-basic"
  ],
	"packageRules": [
		{
			"updateTypes": ["minor", "patch", "pin", "digest"],
			"automerge": true
		}
	]
	"separateMajorMinor": false,
  "lockFileMaintenance": {
    "extends": [
      "group:all"
    ],
    "commitMessageAction": "Update"
  }
}
