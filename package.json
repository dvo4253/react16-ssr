{
  "name": "react16-ssr-template",
  "version": "1.0.0",
  "description": "Template for react 16 ssr app",
  "repository": "https://github.com/dvo4253/react16-ssr.git",
  "scripts": {
    "predev": "rm -rf ./dist",
    "dev": "npm run lint && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:analyze": "BUNDLE_ANALYZER=true npm run dev",
    "dev:server": "NODE_ENV=development BABEL_ENV=development nodemon src/server/index.js --exec babel-node",
    "dev:client": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --config webpack/webpack.dev.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack/webpack.prod.babel.js",
    "build:server": "webpack --config webpack/webpack.server.babel.js",
    "start": "node ./dist/server/index.js",
    "test": "npm run lint && jest --watch",
    "test:ci": "npm run lint && jest --coverage",
    "lint": "eslint --fix ./",
    "init": "source init.sh",
    "clean": "rm -rf node_modules && rm -rf dist",
    "clean:install": "npm run clean && npm install && npm outdated"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-react": "7.23.3",
    "assets-webpack-plugin": "7.1.1",
    "autoprefixer": "10.4.18",
    "axios": "0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.8",
    "eslint": "8.57.0",
    "express": "4.18.3",
    "express-winston": "4.2.0",
    "html-webpack-harddisk-plugin": "2.0.0",
    "html-webpack-plugin": "5.6.0",
    "html-webpack-pug-plugin": "4.0.0",
    "install": "0.13.0",
    "mini-css-extract-plugin": "2.8.1",
    "node-sass": "4.14.1",
    "npm": "10.5.0",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss-loader": "8.1.1",
    "prop-types": "15.8.1",
    "pug": "3.0.0",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redux": "5.0.1",
    "sass-loader": "14.1.1",
    "tar": "6.0.2",
    "terser-webpack-plugin": "5.3.10",
    "webpack": "5.90.3",
    "webpack-disk-plugin": "0.0.2",
    "webpack-merge": "5.10.0",
    "webpack-node-externals": "3.0.0",
    "winston": "3.12.0"
  },
  "devDependencies": {
    "@babel/node": "7.23.9",
    "css-loader": "6.10.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.34.1",
    "jest": "29.7.0",
    "nodemon": "3.1.0",
    "style-loader": "3.3.4",
    "supertest": "6.3.4",
    "wait-on": "7.2.0",
    "webpack-bundle-analyzer": "4.10.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.3"
  }
}
