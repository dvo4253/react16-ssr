{
  "name": "react16-ssr-template",
  "version": "1.0.0",
  "description": "Template for react 16 ssr app",
  "repository": "https://github.com/dvo4253/react16-ssr.git",
  "scripts": {
    "predev": "rm -rf ./dist",
    "dev": "npm run lint && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:analyze": "BUNDLE_ANALYZER=true npm run dev",
    "dev:server": "NODE_ENV=development BABEL_ENV=development nodemon src/server/index.js --exec babel-node",
    "dev:client": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --config webpack/webpack.dev.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack/webpack.prod.babel.js",
    "build:server": "webpack --config webpack/webpack.server.babel.js",
    "start": "node ./dist/server/index.js",
    "test": "npm run lint && jest --watch",
    "test:ci": "npm run lint && jest --coverage",
    "lint": "eslint --fix ./",
    "init": "source init.sh",
    "clean": "rm -rf node_modules && rm -f package-lock.json && rm -rf dist",
    "clean:install": "npm run clean && npm install && npm outdated"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "assets-webpack-plugin": "5.0.2",
    "autoprefixer": "9.8.4",
    "axios": "0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "concurrently": "5.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.3.1",
    "express": "4.17.1",
    "express-winston": "4.0.3",
    "html-webpack-harddisk-plugin": "1.0.2",
    "html-webpack-plugin": "4.3.0",
    "html-webpack-pug-plugin": "2.0.0",
    "isomorphic-fetch": "2.2.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "7.0.2",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prop-types": "15.7.2",
    "pug": "3.0.0",
    "ramda": "0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "redux-thunk": "2.3.0",
    "rxjs": "6.6.0",
    "sass-loader": "9.0.1",
    "terser-webpack-plugin": "3.0.6",
    "webpack": "4.43.0",
    "webpack-disk-plugin": "0.0.2",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2",
    "winston": "3.3.3",
    "write-assets-webpack-plugin": "1.0.5"
  },
  "devDependencies": {
    "@babel/node": "7.10.4",
    "css-loader": "3.6.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.3",
    "jest": "26.1.0",
    "nodemon": "2.0.4",
    "style-loader": "1.2.1",
    "supertest": "4.0.2",
    "wait-on": "5.0.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
