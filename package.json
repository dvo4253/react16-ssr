{
  "name": "react16-ssr-template",
  "version": "1.0.0",
  "description": "Template for react 16 ssr app",
  "repository": "https://github.com/dvo4253/react16-ssr.git",
  "scripts": {
    "babel-node": "babel-node ./src/server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack  --config webpack/webpack.prod.babel.js",
    "build:server": "webpack --config webpack/webpack.server.babel.js",
    "build:dev": "npm run build:client:dev & npm run build:server:dev",
    "build:client:dev": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --config webpack/webpack.dev.babel.js",
    "build:server:dev": "NODE_ENV=development BABEL_ENV=development webpack --config webpack/webpack.server.babel.js --watch",
    "dev": "npm run build:dev & npm run start:dev",
    "dev:server": "nodemon src/server/index.js --exec babel-node",
    "start": "node ./dist/server/index.js",
    "start:dev": "NODE_ENV=development wait-on ./dist/server && nodemon --watch ./dist/server",
    "start:inspect": "npm run lint && npm run build && nodemon --inspect --exec npm run babel-node",
    "test": "npm run lint && jest --watch",
    "test:ci": "npm run lint && jest --coverage",
    "lint": "eslint --fix ./",
    "init": "source init.sh",
    "clean": "rm -rf node_modules && rm -rf dist",
    "clean:install": "npm run clean && npm install && npm outdated"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assets-webpack-plugin": "3.9.5",
    "autoprefixer": "9.1.1",
    "axios": "0.18.0",
    "babel-loader": "7.1.5",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "4.19.1",
    "express": "4.16.3",
    "express-winston": "2.5.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-pug-plugin": "0.3.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "23.5.0",
    "node-sass": "4.9.3",
    "prop-types": "15.6.2",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "ramda": "0.25.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.4",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-observable": "1.0.0",
    "rxjs": "6.2.2",
    "sass-loader": "7.1.0",
    "webpack": "4.16.5",
    "webpack-disk-plugin": "0.0.2",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2",
    "winston": "2.4.3",
    "write-assets-webpack-plugin": "1.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "css-loader": "1.0.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "nodemon": "1.18.3",
    "style-loader": "0.22.1",
    "supertest": "3.1.0",
    "wait-on": "2.1.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  }
}
